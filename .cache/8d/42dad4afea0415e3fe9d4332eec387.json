,"generated":{"line":95,"column":0},"source":"app.js","original":{"line":45,"column":0}},{"generated":{"line":95,"column":4},"source":"app.js","original":{"line":45,"column":4}}],"sources":{"app.js":"import axios from 'axios';\n\nconst BASE_URL = 'https://jsonplaceholder.typicode.com';\n\nconst  getTodos = async () => {\n  try {\n    const { data } = await axios.get(`{BASE_URL}/todos`);\n\n    const todos = data;\n\n    console.log(`GET: Here's the list of todos `, todos);\n\n    return todos;\n  } catch (error) {\n    console.error(error);\n  }\n}\n\nconst createLi = item => { \n  const li = document.createElement('li');\n\n  li.appendChild(document.createTextNode(item.title));\n\n  return li;\n};\n\n\nconst addTodosToDom = todos => { \n  const ul = document.querySelector('ul');\n\n  if(Array.isArray(todos) && todos.length > 0){ \n    todos.map(todo => { \n      ul.appendChild(createLi(todo));\n    });\n  }\n  else if(todos) {\n    ul.appendChild(createLi(todos));\n  }\n};\n\nconst main = async () => { \n  addTodosToDom(await getTodos());\n}; \n\nmain();"},"lineCount":null}},"error":null,"hash":"5fde2a07a0da157d8e2e4378d9866d66","cacheData":{"env":{}}}